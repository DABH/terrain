{
  "name": "terrain-search",
  "version": "1.0.0",
  "description": "The one search front-end to rule them all.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "nodemon --exec ./node_modules/.bin/ts-node -P midway $NODE_DEBUG_OPTION --harmony --max_executable_size=8192 --max_old_space_size=8192 -- ./midway/src/Midway.ts",
    "start-front": "yarn run commit-log && webpack-dev-server --config webpack.config.js --inline --content-base src/app --history-api-fallback --compress --progress --info",
    "test": "karma start karma.conf.js",
    "test-front": "jest src",
    "test-back-setup": "./midway/test/scripts/setup_env.sh",
    "test-back": "jest midway shared --forceExit --coverage",
    "test-back-teardown": "./midway/test/scripts/teardown_env.sh",
    "test-back-all": "yarn run test-back-setup && yarn run test-back ; yarn run test-back-teardown",
    "commit-log": "git log --pretty=format:\"%h%x09%an%x09%ad%x09%s\" | head -n 10 > src/commitlog.txt",
    "build-prod": "yarn run commit-log && webpack --config webpack.config.prod.js --output-filename ./midway/src/assets/bundle.js",
    "build": "yarn run commit-log && webpack --config webpack.config.js --output-filename ./midway/src/assets/bundle.js",
    "lint": "yarn run lint-front && yarn run lint-back",
    "lint-parallel": "npm-run-all --parallel lint-front lint-back",
    "lint-back": "tslint -c tslint.json --fix --type-check --project midway/tsconfig.json",
    "lint-front": "tslint -c tslint.json --fix --type-check --project tsconfig.json",
    "style": "find midway src shared -type f \\( -iname \\*.tsx -o -iname \\*.ts \\) | parallel --no-notice ./node_modules/.bin/tsfmt -r {}\\; | grep replaced || exit 0 && exit 1",
    "fix": "yarn run style && yarn run lint-parallel;",
    "diff-style": "git ls-files -m  \\*.ts\\* | parallel --no-notice ./node_modules/.bin/tsfmt -r {}\\; | grep replaced || exit 0 && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://git.terrain.int/terrain/Search"
  },
  "authors": [
    "Han Solo"
  ],
  "pre-commit": [
    "lint",
    "style"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.+Tests\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "mapCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|svg\\?.*|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/mocks/fileMock.ts",
      "^auth/(.*)$": "<rootDir>/src/app/auth/$1",
      "^analytics/(.*)$": "<rootDir>/src/app/analytics/$1",
      "^app/(.*)$": "<rootDir>/src/app/$1",
      "^builder/(.*)$": "<rootDir>/src/app/builder/$1",
      "^charts/(.*)$": "<rootDir>/src/app/charts/$1",
      "^common/(.*)$": "<rootDir>/src/app/common/$1",
      "^control/(.*)$": "<rootDir>/src/app/control/$1",
      "^database/(.*)$": "<rootDir>/src/database/$1",
      "^deploy/(.*)$": "<rootDir>/src/app/deploy/$1",
      "^fileImport/(.*)$": "<rootDir>/src/app/fileImport/$1",
      "^images/(.*)$": "<rootDir>/src/images/$1",
      "^library/(.*)$": "<rootDir>/src/app/library/$1",
      "^manual/(.*)$": "<rootDir>/src/app/manual/$1",
      "^roles/(.*)$": "<rootDir>/src/app/roles/$1",
      "^schema/(.*)$": "<rootDir>/src/app/schema/$1",
      "^shared/(.*)$": "<rootDir>/shared/$1",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^store/(.*)$": "<rootDir>/src/app/store/$1",
      "^tql/(.*)$": "<rootDir>/src/app/tql/$1",
      "^users/(.*)$": "<rootDir>/src/app/users/$1",
      "^util/(.*)$": "<rootDir>/src/app/util/$1",
      "^x/(.*)$": "<rootDir>/src/app/x/$1",
      "^.+\\.(css|less|scss)$": "ts-jest"
    },
    "globals": {
      "DEV": true,
      "MIDWAY_HOST": "http://localhost"
    },
    "setupFiles": [
      "<rootDir>/src/test/mocks/browserMock.ts"
    ],
    "testURL": "http://localhost"
  },
