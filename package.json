{
  "name": "terrain-search",
  "version": "1.0.0",
  "description": "The one search front-end to rule them all.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "yarn start-postgres && yarn start-midway",
    "start-minified": "yarn start-postgres && yarn start-midway-minified",
    "start-midway": "rm -rf midway/build; mkdir midway/build; touch midway/build/build.log; ( yarn build-server-live & ) && while ! grep -q \"Compilation complete. Watching for file changes.\" midway/build/build.log; do sleep 1; done && NODE_PATH=${NODE_PATH}:./midway/build nodemon -e js --watch midway/build -r source-map-support/register --max_old_space_size=8192 ./midway/build/midway/src/Midway.js",
    "start-midway-minified": "node ./midway/src/assets/bundle.server.js",
    "start-postgres": "docker-compose -f midway-postgres-docker.yml up -d",
    "start-front": "yarn commit-log && webpack-dev-server --config webpack.config.js --inline --content-base midway/src/assets --history-api-fallback --compress --progress --info --mode development",
    "stop-postgres": "docker-compose -f midway-postgres-docker.yml down",
    "test": "yarn test-back && yarn test-front",
    "test-front": "jest src --forceExit --coverage --detectOpenHandles",
    "test-back-setup": "./midway/test/scripts/setup_env.sh --use-chrome=0",
    "test-sigint-setup": "./midway/test/scripts/setup_env.sh --use-mysql=0 --use-sqlite=0 --use-chrome=0",
    "test-back": "jest midway shared --forceExit --coverage",
    "test-back-teardown": "./midway/test/scripts/teardown_env.sh --use-chrome=0",
    "test-sigint-teardown": "./midway/test/scripts/teardown_env.sh --use-mysql=0 --use-sqlite=0 --use-chrome=0",
    "test-back-all": "yarn test-back-setup && yarn test-back ; yarn test-back-teardown",
    "test-fullstack-setup": "./midway/test/scripts/setup_env.sh --postgres-port=5432 --postgres-db=midway --use-mysql=0 --use-sqlite=0",
    "test-fullstack-teardown": "./midway/test/scripts/teardown_env.sh --use-mysql=0 --use-sqlite=0",
    "commit-log": "git log --pretty=format:\"%h%x09%an%x09%ad%x09%s\" | head -n 10 > src/commitlog.txt",
    "copy-assets": "cp src/app/index.html midway/src/assets/index.html",
    "build": "yarn commit-log && webpack --config webpack.config.js --output-path midway/src/bundles/ --mode development",
    "build-prod": "yarn commit-log && webpack -p --config webpack.config.prod.js --output-path midway/src/bundles/ --mode production",
    "build-server-minified": "webpack -p --config webpack.config.server.js --output-path midway/src/assets/ --mode production",
    "build-server": "tsc --noEmitOnError -p midway/tsconfig.json",
    "build-server-live": "yarn build-server -w | tee midway/build/build.log | sed 's/\\x1b//g'",
    "lint": "yarn lint-front && yarn lint-back && yarn lint-sigint && yarn lint-analytics-js",
    "lint-parallel": "npm-run-all --parallel lint-front lint-back lint-sigint lint-analytics-js",
    "lint-front": "tslint -c tslint.json --fix --project tsconfig.json",
    "lint-back": "tslint -c tslint.json --fix --project midway/tsconfig.json",
    "lint-sigint": "tslint -c sigint/tslint.json --fix --project sigint/tsconfig.json",
    "lint-analytics-js": "( cd analytics.js && tslint -c tslint.json --fix --project tsconfig.json )",
    "style": "git ls-files | grep \"\\.ts\" | parallel --no-notice ./node_modules/.bin/tsfmt -r {} | grep replaced || exit 0 && exit 1",
    "style-branch": "run(){ git diff --name-only HEAD $(git merge-base HEAD $1) | egrep '.ts$|.tsx$' | parallel --no-notice ./node_modules/.bin/tsfmt -r {}\\; | grep replaced || exit 0 && exit 1; }; run",
    "fix": "yarn style && yarn lint-parallel;",
    "diff-style": "git ls-files -m | grep \"\\.ts\" | parallel --no-notice ./node_modules/.bin/tsfmt -r {} | grep replaced || exit 0 && exit 1",
    "rr": "tsc --noEmitOnError -p rr/tsconfig.json && NODE_PATH=${NODE_PATH}:./rr/build node ./rr/build/rr/RecordReplay.js",
    "fetch-query": "tsc --noEmitOnError -p rr/tsconfig.json && NODE_PATH=${NODE_PATH}:./rr/build node ./rr/build/rr/FetchQueries.js",
    "test-fullstack-login": "jest ./rr/test/login --forceExit",
    "test-fullstack-import": "jest ./rr/test/import --forceExit",
    "test-rr-pathfinder": "jest ./rr/test/pathfinder --forceExit",
    "test-rr-etl-http-import": "jest ./rr/test/etl --forceExit",
    "test-fullstack-component": "jest ./rr/test/components --forceExit",
    "clean": "rm -rf .cache && rm -rf .cache-loader && rm -rf midway/src/bundles/.cache"
  },
  "repository": {
    "type": "git",
    "url": "http://git.terrain.int/terrain/Search"
  },
  "authors": [
    "Han Solo"
  ],
  "pre-commit": [
    "lint",
    "diff-style"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": ["node_modules", "midway/build", "rr/build"],
    "testRegex": "/test/.+Tests\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|svg\\?.*|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/__mocks__/fileMock.ts",
      "^.+\\.less$": "<rootDir>/src/test/__mocks__/styleMock.ts",
      "^auth/(.*)$": "<rootDir>/src/app/auth/$1",
      "^analytics/(.*)$": "<rootDir>/src/app/analytics/$1",
      "^app/(.*)$": "<rootDir>/src/app/$1",
      "^builder/(.*)$": "<rootDir>/src/app/builder/$1",
      "^charts/(.*)$": "<rootDir>/src/app/charts/$1",
      "^common/(.*)$": "<rootDir>/src/app/common/$1",
      "^control/(.*)$": "<rootDir>/src/app/control/$1",
      "^database/(.*)$": "<rootDir>/src/database/$1",
      "^deploy/(.*)$": "<rootDir>/src/app/deploy/$1",
      "^etl/(.*)$": "<rootDir>/src/app/etl/$1",
      "^images/(.*)$": "<rootDir>/src/images/$1",
      "^jobs/(.*)$": "<rootDir>/src/app/jobs/$1",
      "^library/(.*)$": "<rootDir>/src/app/library/$1",
      "^manual/(.*)$": "<rootDir>/src/app/manual/$1",
      "^roles/(.*)$": "<rootDir>/src/app/roles/$1",
      "^scheduler/(.*)$": "<rootDir>/src/app/scheduler/$1",
      "^schema/(.*)$": "<rootDir>/src/app/schema/$1",
      "^shared/(.*)$": "<rootDir>/shared/$1",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^store/(.*)$": "<rootDir>/src/app/store/$1",
      "^tql/(.*)$": "<rootDir>/src/app/tql/$1",
      "^users/(.*)$": "<rootDir>/src/app/users/$1",
      "^util/(.*)$": "<rootDir>/src/app/util/$1",
      "^x/(.*)$": "<rootDir>/src/app/x/$1",
      "^test-helpers/(.*)$": "<rootDir>/src/test/test-helpers/$1",
      "^.+\\.(css|less|scss)$": "ts-jest"
    },
    "globals": {
      "DEV": true
    },
    "setupFiles": [
      "<rootDir>/src/test/__mocks__/browserMock.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/setup.ts",
    "testURL": "http://localhost",
    "clearMocks": true
  },
