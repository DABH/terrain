{
  "name": "terrain-search",
  "version": "1.0.0",
  "description": "The one search front-end to rule them all.",
  "main": "index.js",
  "scripts": {
    "start": "yarn run $NODE_DEBUG_OPTION build-midway:live",
    "start-front": "yarn run commit-log && MIDWAY_HOST=localhost:3000 OLD_MIDWAY_HOST=pa-terraformer02.terrain.int webpack-dev-server --config webpack.config.js --inline --content-base src/app --history-api-fallback --compress",
    "test": "karma start karma.conf.js",
    "test-front": "only=front karma start karma.conf.js ",
    "test-back-setup": "./midway/test/scripts/setup_env.sh",
    "test-back": "./node_modules/.bin/jest --forceExit --coverage",
    "test-back-teardown": "./midway/test/scripts/teardown_env.sh",
    "test-back-all": "yarn run test-back-setup && yarn run test-back ; yarn run test-back-teardown",
    "commit-log": "git log --pretty=format:\"%h%x09%an%x09%ad%x09%s\" | head -n 10 > src/commitlog.txt",
    "build-prod": "yarn run commit-log && MIDWAY_HOST=localhost:3000 webpack --config webpack.config.prod.js --output-filename ./midway/src/assets/bundle.min.js",
    "build": "yarn run commit-log && MIDWAY_HOST=localhost:3000 webpack --config webpack.config.js --output-filename ./midway/src/assets/bundle.js",
    "build-midway:live": "nodemon --exec ./node_modules/.bin/ts-node -P midway --harmony -- ./midway/src/Midway.ts",
    "lint": "yarn run lint-front && yarn run lint-back",
    "lint-back": "./node_modules/.bin/tslint -c tslint.json --fix --type-check --project midway/tsconfig.json",
    "lint-front": "./node_modules/.bin/tslint -c tslint.json --fix --type-check --project tsconfig.json",
    "style": "find midway src shared -type f \\( -iname \\*.tsx -o -iname \\*.ts \\) | parallel --no-notice ./node_modules/.bin/tsfmt -r {}\\;",
    "fix": "yarn run lint && yarn run style;"
  },
  "repository": {
    "type": "git",
    "url": "http://git.terrain.int/terrain/Search"
  },
  "authors": [
    "Han Solo"
  ],
  "pre-commit": [
    "lint",
    "style"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.+Tests\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "mapCoverage": true
  },
  "private": true,
