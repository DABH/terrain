  "scripts": {
    "start": "npm run build-midway:live",
    "start-old": "MIDWAY_HOST=pa-terraformer01.terrain.int webpack-dev-server --config webpack.config.js --inline --content-base src/app --history-api-fallback",
    "start2": "MIDWAY_HOST=pa-terraformer02.terrain.int webpack-dev-server --config webpack.config.js --inline --content-base src/app --history-api-fallback",
    "start-koa": "MIDWAY_HOST=localhost:3000 webpack-dev-server --config webpack.config.js --inline --content-base src/app --history-api-fallback",
    "start-local": "MIDWAY_HOST=localhost webpack-dev-server --config webpack.config.js --inline --content-base src/app --history-api-fallback",
    "test": "karma start karma.conf.js",
    "test-front": "only=front karma start karma.conf.js ",
    "test-back": "./node_modules/.bin/ts-node --harmony ./node_modules/.bin/tape midway/test/TestSuite.tsx",
    "deploy": "MIDWAY_HOST=pa-terraformer01.terrain.int webpack --config webpack.config.js --output-filename bundle.js && scp bundle.js pa-terraformer01.terrain.int:/home/lknepper/midway/dist/bundle.js",
    "deploy2": "MIDWAY_HOST=pa-terraformer02.terrain.int webpack --config webpack.config.js --output-filename bundle.js && scp bundle.js pa-terraformer02.terrain.int:midway/dist/bundle.js",
    "commit-log": "git log | head -n 100 > src/commitlog.txt",
    "build-prod": "npm run commit-log && MIDWAY_HOST=pa-terraformer01.terrain.int webpack --config webpack.config.prod.js --output-filename bundle.js",
    "build-demo": "npm run commit-log && MIDWAY_HOST=localhost webpack --config webpack.config.prod.js --output-filename bundle.demo.js",
    "build-gcloud": "npm run commit-log && webpack --config webpack.config.gcloud.js --output-filename bundle.js",
    "build-midway:live": "nodemon --exec ./node_modules/.bin/ts-node --harmony -- ./midway/App.tsx"
  },
